include lib/std/io 

fun divby
	mod 0 =? ;

act fizzbuzz 
	var max
	1 var x
	loop: $x $max >? then: break ;
		elect: 
			$x 15 divby then: "FizzBuzz!" prints ;
			$x  3 divby then: "Fizz" prints ;
			$x  5 divby then: "Buzz" prints ;
			$x printi ;
		"\n" prints
		$x 1 + x! 
	:loop
:act

act main 100 fizzbuzz ;
